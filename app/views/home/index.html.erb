<div class="home_page">
  <h1 class="header">MeetUp Category VS Weather</h1>

  <%= form_tag do %>
    <%= label_tag(:city, "Select city:") %>
    <%= text_field_tag(:city) %>
    <%= label_tag(:state, "Select state:") %>
    <span class="state">

      <%= select_tag :state, options_for_select(us_states) %>
    </span>
    <%= submit_tag("Process") %>
  <% end %>
</div>


